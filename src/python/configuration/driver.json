{
    "bucket": "serverless-mapreduce-storage",
    "prefix": "test_input_data/",
    "jobBucket": "serverless-mapreduce-storage",
    "region": "us-east-1",
    "lambdaMemory": 1536,
    "concurrentLambdas": 1000,
    "lambda_read_timeout": 300,
    "boto_max_connections": 1000,
    "num_reducers": 4,
    "mapper": {
        "name": "job/map.py",
        "handler": "job/map.map_function",
        "zip": "map.zip"
    },
    "reducer":{
        "name": "job/reduce.py",
        "handler": "job/reduce.reduce_function",
        "zip": "reduce.zip"
    },
    "reducerCoordinator":{
        "name": "coordinator/reduce_coordinator.py",
        "handler": "coordinator/reduce_coordinator.lambda_handler",
        "zip": "reduce_coordinator.zip"
    }
}
