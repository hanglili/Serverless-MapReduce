{
    "jobBucket": "serverless-mapreduce-storage",
    "region": "us-east-1",
    "lambdaMemory": 1536,
    "concurrentLambdas": 1000,
    "lambdaReadTimeout": 300,
    "botoMaxConnections": 1000,
    "numReducers": 4,
    "mapper": {
        "name": "job/map.py",
        "handler": "job/map.map_function",
        "zip": "map.zip"
    },
    "reducer": {
        "name": "job/reduce.py",
        "handler": "job/reduce.reduce_function",
        "zip": "reduce.zip"
    },
    "reducerCoordinator": {
        "name": "coordinator/reduce_coordinator.py",
        "handler": "coordinator/reduce_coordinator.lambda_handler",
        "zip": "reduce_coordinator.zip"
    },
    "driver": {
        "name": "driver/serverless_driver.py",
        "handler": "driver/serverless_driver.lambda_handler",
        "zip": "serverless_driver.zip"
    }
}
